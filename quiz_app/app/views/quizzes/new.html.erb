<%= form_for @quiz do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title, placeholder: 'Please add your title' %>

	 
	  <%= f.fields_for :questions do |question_f| %>
	    <%= question_f.label :query, "Question" %>
		<%= question_f.text_field :query, placeholder: 'Please add your question' %>
	  <% end %>

	  

  <%= f.submit %>

<% end %>






